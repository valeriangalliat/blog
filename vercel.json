{
  "github": {
    "silent": true
  },
  "ignoreCommand": "if [ \"$VERCEL_ENV\" == \"production\" ]; then exit 1; else exit 0; fi",
  "redirects": [
    {
      "source": "/resources/git.html",
      "destination": "/git.html",
      "permanent": true
    },
    {
      "source": "/resources/vim.html",
      "destination": "/vim.html",
      "permanent": true
    },
    {
      "source": "/resources/simple.html",
      "destination": "/simple.html",
      "permanent": true
    },
    {
      "source": "/2025/04/turborepo-bugger-logs-github-actions.html",
      "destination": "/2025/04/turborepo-buffer-logs-github-actions.html",
      "permanent": true
    },
    {
      "source": "/now.html",
      "destination": "/2026/01/the-next-chapter.html",
      "permanent": false
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "val.codejam.info"
        }
      ],
      "destination": "https://www.codejam.info/val.html"
    }
  ]
}
